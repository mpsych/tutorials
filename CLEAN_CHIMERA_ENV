conda create --name TF25 python=3.9.7

conda activate TF25

conda install -c conda-forge cudatoolkit=11.3.1=h2bc3f7f_2

conda install -c conda-forge cudnn=8.2.1.32=h86fa8c9_0

pip install tensorflow==2.5.0

pip install keras==2.5.0rc0

conda install jupyter


